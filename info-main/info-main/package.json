    "sass": "^1.69.5",
    "sass-loader": "^13.3.2",
    "tailwindcss-debug-screens": "^2.2.1",
    "typescript": "^5.2.2",
    "vue": "^3.3.8",
    "vue-router": "^4.2.5",
    "vue-tsc": "^1.8.22"
  },
  "dependencies": {
    "crisp-sdk-web": "^1.0.21",
    "luxon": "^3.4.4"
  }

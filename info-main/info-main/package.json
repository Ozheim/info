{
  "name": "izidoor",
  "repository": {
    "type": "git",
    "url": "https://github.com/sportoutdoor/izidoor"
  },
  "author": "Izidoor",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev --open",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "prepare": "husky install && nuxt prepare",
    "lint:js": "eslint --fix --ext \".js,.ts,.vue\" --ignore-path .gitignore .",
    "lintfix": "prettier --write --list-different . && yarn lint:js --fix"
